    <Ingredients>
      <Grain>
        <F_G_NAME>Pale Malt, Maris Otter</F_G_NAME>
        <F_G_AMOUNT>5.0</F_G_AMOUNT>
        <F_G_COLOR>3.0</F_G_COLOR>
