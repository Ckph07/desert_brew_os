        <F_H_ALPHA>12.0</F_H_ALPHA>
      </Hops>
      <Yeast>
        <F_Y_NAME>Safale US-05</F_Y_NAME>
        <F_Y_LAB>Fermentis</F_Y_LAB>
        <F_Y_PRODUCT_ID>US-05</F_Y_PRODUCT_ID>
        <F_Y_TYPE>Ale</F_Y_TYPE>
      </Yeast>
    </Ingredients>
