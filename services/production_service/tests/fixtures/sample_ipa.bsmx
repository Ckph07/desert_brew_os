    <F_R_BREWER>Desert Brew Co</F_R_BREWER>
    <F_R_OG>1.065</F_R_OG>
    <F_R_FG>1.012</F_R_FG>
    <F_R_ABV>6.96</F_R_ABV>
    <F_R_IBU>65.0</F_R_IBU>
    <F_R_COLOR>8.5</F_R_COLOR>
